{
  "name": "react-typescript-skeleton",
  "version": "0.0.1",
  "description": "React typescript ready to use skeleton with eslint, webpack",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.config.ts",
    "build": "webpack --config webpack/webpack.prod.config.ts",
    "test": "jest --config jest/jest.config.ts --watchAll",
    "lint": "eslint src/*",
    "prettier": "prettier --check"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.8",
    "@testing-library/react": "^12.1.4",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn pretter:check",
      "yarn lint"
    ]
  }
}
